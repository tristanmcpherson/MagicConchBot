<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <AssemblyVersion>1.1.1.0</AssemblyVersion>
    <Version>1.1.0.0</Version>
    <PrimaryVersionType>AssemblyVersionAttribute</PrimaryVersionType>
    <UpdateAssemblyVersion>True</UpdateAssemblyVersion>
    <UpdateAssemblyFileVersion>False</UpdateAssemblyFileVersion>
    <UpdateAssemblyInfoVersion>False</UpdateAssemblyInfoVersion>
    <AssemblyVersionSettings>None.None.None.Increment</AssemblyVersionSettings>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="temp\**" />
    <Compile Remove="Wiki\**" />
    <EmbeddedResource Remove="temp\**" />
    <EmbeddedResource Remove="Wiki\**" />
    <None Remove="temp\**" />
    <None Remove="Wiki\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="application.log" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="1.0.0-rc-00649" />
    <PackageReference Include="Google.Apis.YouTube.v3" Version="1.22.0.760" />
    <PackageReference Include="HtmlAgilityPack.NetCore" Version="1.5.0.1" />
    <PackageReference Include="NLog" Version="5.0.0-beta06" />
    <PackageReference Include="SoundCloud.API.Client.NetCore" Version="1.0.0-CI00007" />
    <PackageReference Include="System.Diagnostics.FileVersionInfo" Version="4.3.0" />
    <PackageReference Include="Wiki.Net" Version="1.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="FChan-Core">
      <HintPath>..\..\..\..\..\..\Documents\Visual Studio 2017\Projects\FChan-Core\FChan-Core\bin\Debug\netcoreapp1.1\FChan-Core.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>